class Arr
{
	public static void main(String[] args)
	{
		int[] arr={5,4,3,6,9,8,7};
		for (int i=0;i<=arr.length;i++)
		{
			for (int j=0;j<=arr.length;j++)
			{
				if (arr[i]<arr[j])
				{
					int temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
			
		}
		for(int i:arr)
		{
			System.out.println(i+" ");
		}
		
	}
}